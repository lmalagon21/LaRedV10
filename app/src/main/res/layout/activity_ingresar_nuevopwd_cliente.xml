<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ingresar_nuevopwd_cliente">
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/bienvenido_a_lared"
            android:id="@+id/estas_enred"
            android:textColor="#009688" android:textColorHighlight="#E91E63"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <TextView
            android:text="Digite su nueva conseña"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/label_newpwd_digitar" android:textColor="#009688"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="46dp"
            app:layout_constraintTop_toBottomOf="@+id/estas_enred" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Número Móvil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/label_movil_newpwd_cliente"
            android:textColor="#009688" app:layout_constraintStart_toStartOf="@+id/label_newpwd_digitar"
            android:layout_marginTop="47dp" android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            app:layout_constraintTop_toBottomOf="@+id/label_newpwd_digitar"/>

</androidx.constraintlayout.widget.ConstraintLayout>